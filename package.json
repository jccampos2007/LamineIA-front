{
  "name": "lamine-developer-help",
  "displayName": "lamine Developer Help",
  "description": "Ayuda para desarrolladores de lamine",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:lamine-developer-help.listFiles"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lamine-developer-help.listFiles",
        "title": "Mostrar Archivos",
        "icon": "$(file-directory)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lamineViewContainer",
          "title": "Lamine",
          "icon": "./resources/image/mi-icono.svg"
        }
      ]
    },
    "views": {
      "lamineViewContainer": [
        {
          "id": "lamine-developer-help.listFiles",
          "name": "Explorador de Archivos",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -w .",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/dotenv": "^6.1.1",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.1.0",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vscode/test-electron": "^2.3.9",
    "chai": "^4.3.10",
    "dotenv": "^16.5.0",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "sinon": "^18.0.0",
    "typescript": "^5.3.3"
  }
}
